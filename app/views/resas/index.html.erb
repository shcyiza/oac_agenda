

<h1>Listing Resas</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Event</th>
      <th>Resanr</th>
      <th>Resa claimed</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resas.each do |resa| %>
      <tr>
        <td><%= resa.user_id %></td>
        <td><%= resa.event_id %></td>
        <td><%= resa.resanr %></td>
        <td><%= resa.resa_claimed %></td>
        <td><%= link_to 'Show', resa %></td>
        <td><%= link_to 'Edit', edit_resa_path(resa) %></td>
        <td><%= link_to 'Destroy', resa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Resa', new_resa_path %>
